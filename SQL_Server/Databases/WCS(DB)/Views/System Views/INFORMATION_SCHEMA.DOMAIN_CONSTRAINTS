/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [CONSTRAINT_CATALOG]
      ,[CONSTRAINT_SCHEMA]
      ,[CONSTRAINT_NAME]
      ,[DOMAIN_CATALOG]
      ,[DOMAIN_SCHEMA]
      ,[DOMAIN_NAME]
      ,[IS_DEFERRABLE]
      ,[INITIALLY_DEFERRED]
  FROM [WCS].[INFORMATION_SCHEMA].[DOMAIN_CONSTRAINTS]
