/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [IF_DOCNO]
      ,[IFTIME]
      ,[IFFLAG]
      ,[SRC_SYSTEM]
      ,[COMMANDID]
      ,[COMMANDTYPE]
      ,[REQUESTDATE]
      ,[REMOVE_PRGS_CD]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
      ,[REMARK]
  FROM [WCS].[dbo].[IF_MAIN_DEL_ORD_HIST]
