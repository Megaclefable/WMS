/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [OPC_SERVER_CD]
      ,[NODE_ID]
      ,[NODE_ID_HEX]
      ,[NODE_DESC]
      ,[EQP_ID]
      ,[TYPE]
      ,[SIGNAL_TYPE]
      ,[READ_YN]
      ,[WRITE_YN]
      ,[MONITOR_YN]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WCS_CFG_OPC_NODE_ID_20230317]
