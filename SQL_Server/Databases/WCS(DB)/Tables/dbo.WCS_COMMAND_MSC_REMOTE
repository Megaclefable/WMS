/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [EQP_ID]
      ,[ACTIVE]
      ,[E_STOP]
      ,[STOP]
      ,[ERROR_RESET]
      ,[HOME_RETURN]
      ,[F1_DATA_DEL]
      ,[F2_DATA_DEL]
      ,[F1_F2_DATA_DEL]
      ,[EMPTY_ERROR_RESET]
      ,[COMMAND_STATUS]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WCS_COMMAND_MSC_REMOTE]
