/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [EQP_TYPE]
      ,[DEVICE_DATA_NM]
      ,[START_WORD]
      ,[START_BIT]
      ,[LENGTH]
      ,[READ_YN]
      ,[WRITE_YN]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WCS_CFG_CV_DEVICE_MAP]
