/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [EQP_ID]
      ,[EQP_TYPE]
      ,[EQP_GROUP]
      ,[EQP_GUBUN]
      ,[CV_POLLING_GROUP]
      ,[CV_TRACK_NO]
      ,[CV_DEST_NO]
      ,[INTERFACE_ID]
      ,[DISPLAY_NM]
      ,[BUFFER_SEQ]
      ,[CONTAINER_EXIST]
      ,[REMARK]
      ,[OPC_SERVER_CD]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WCS_CFG_EQP_ID]
