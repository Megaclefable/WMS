/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [memory_consumer_id]
      ,[memory_consumer_type]
      ,[memory_consumer_type_desc]
      ,[memory_consumer_desc]
      ,[object_id]
      ,[xtp_object_id]
      ,[index_id]
      ,[allocated_bytes]
      ,[used_bytes]
      ,[allocation_count]
      ,[partition_count]
      ,[sizeclass_count]
      ,[min_sizeclass]
      ,[max_sizeclass]
      ,[memory_consumer_address]
  FROM [master].[sys].[dm_db_xtp_memory_consumers]
