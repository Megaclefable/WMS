/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [cycle_id]
      ,[ticks_at_cycle_start]
      ,[ticks_at_cycle_end]
      ,[node_id]
      ,[base_generation]
      ,[xacts_copied_to_local]
      ,[xacts_in_gen_0]
      ,[xacts_in_gen_1]
      ,[xacts_in_gen_2]
      ,[xacts_in_gen_3]
      ,[xacts_in_gen_4]
      ,[xacts_in_gen_5]
      ,[xacts_in_gen_6]
      ,[xacts_in_gen_7]
      ,[xacts_in_gen_8]
      ,[xacts_in_gen_9]
      ,[xacts_in_gen_10]
      ,[xacts_in_gen_11]
      ,[xacts_in_gen_12]
      ,[xacts_in_gen_13]
      ,[xacts_in_gen_14]
      ,[xacts_in_gen_15]
  FROM [master].[sys].[dm_db_xtp_gc_cycle_stats]
