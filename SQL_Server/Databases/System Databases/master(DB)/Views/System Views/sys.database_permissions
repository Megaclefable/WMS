/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [class]
      ,[class_desc]
      ,[major_id]
      ,[minor_id]
      ,[grantee_principal_id]
      ,[grantor_principal_id]
      ,[type]
      ,[permission_name]
      ,[state]
      ,[state_desc]
  FROM [master].[sys].[database_permissions]
