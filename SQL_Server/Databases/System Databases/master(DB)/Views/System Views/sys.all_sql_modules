/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [object_id]
      ,[definition]
      ,[uses_ansi_nulls]
      ,[uses_quoted_identifier]
      ,[is_schema_bound]
      ,[uses_database_collation]
      ,[is_recompiled]
      ,[null_on_null_input]
      ,[execute_as_principal_id]
      ,[uses_native_compilation]
      ,[inline_type]
      ,[is_inlineable]
  FROM [master].[sys].[all_sql_modules]
