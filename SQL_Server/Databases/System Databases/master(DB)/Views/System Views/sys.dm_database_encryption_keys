/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [database_id]
      ,[encryption_state]
      ,[create_date]
      ,[regenerate_date]
      ,[modify_date]
      ,[set_date]
      ,[opened_date]
      ,[key_algorithm]
      ,[key_length]
      ,[encryptor_thumbprint]
      ,[encryptor_type]
      ,[percent_complete]
      ,[encryption_state_desc]
      ,[encryption_scan_state]
      ,[encryption_scan_state_desc]
      ,[encryption_scan_modify_date]
  FROM [master].[sys].[dm_database_encryption_keys]
