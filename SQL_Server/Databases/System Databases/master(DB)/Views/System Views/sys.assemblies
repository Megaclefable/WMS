/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [name]
      ,[principal_id]
      ,[assembly_id]
      ,[clr_name]
      ,[permission_set]
      ,[permission_set_desc]
      ,[is_visible]
      ,[create_date]
      ,[modify_date]
      ,[is_user_defined]
  FROM [master].[sys].[assemblies]
