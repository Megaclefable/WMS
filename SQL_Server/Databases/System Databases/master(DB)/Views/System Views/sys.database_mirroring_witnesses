/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [database_name]
      ,[principal_server_name]
      ,[mirror_server_name]
      ,[safety_level]
      ,[safety_level_desc]
      ,[safety_sequence_number]
      ,[role_sequence_number]
      ,[mirroring_guid]
      ,[family_guid]
      ,[is_suspended]
      ,[is_suspended_sequence_number]
      ,[partner_sync_state]
      ,[partner_sync_state_desc]
  FROM [master].[sys].[database_mirroring_witnesses]
