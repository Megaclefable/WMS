/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [conversation_id]
      ,[is_initiator]
      ,[to_service_name]
      ,[to_broker_instance]
      ,[from_service_name]
      ,[from_broker_instance]
      ,[adjacent_broker_address]
      ,[message_sequence_number]
      ,[message_fragment_number]
      ,[hops_remaining]
      ,[time_to_live]
      ,[time_consumed]
      ,[message_id]
  FROM [master].[sys].[dm_broker_forwarded_messages]
