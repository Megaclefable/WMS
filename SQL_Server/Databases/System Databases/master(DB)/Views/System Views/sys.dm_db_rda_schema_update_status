/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [table_id]
      ,[database_id]
      ,[task_id]
      ,[task_type]
      ,[task_type_desc]
      ,[task_state]
      ,[task_state_desc]
      ,[start_time_utc]
      ,[end_time_utc]
      ,[error_number]
      ,[error_severity]
      ,[error_state]
  FROM [master].[sys].[dm_db_rda_schema_update_status]
