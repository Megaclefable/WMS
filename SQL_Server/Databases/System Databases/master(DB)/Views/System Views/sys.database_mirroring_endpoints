/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [name]
      ,[endpoint_id]
      ,[principal_id]
      ,[protocol]
      ,[protocol_desc]
      ,[type]
      ,[type_desc]
      ,[state]
      ,[state_desc]
      ,[is_admin_endpoint]
      ,[role]
      ,[role_desc]
      ,[is_encryption_enabled]
      ,[connection_auth]
      ,[connection_auth_desc]
      ,[certificate_id]
      ,[encryption_algorithm]
      ,[encryption_algorithm_desc]
  FROM [master].[sys].[database_mirroring_endpoints]
