/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [object_id]
      ,[index_id]
      ,[partition_number]
      ,[row_group_id]
      ,[delta_store_hobt_id]
      ,[state]
      ,[state_desc]
      ,[total_rows]
      ,[deleted_rows]
      ,[size_in_bytes]
      ,[trim_reason]
      ,[trim_reason_desc]
      ,[transition_to_compressed_state]
      ,[transition_to_compressed_state_desc]
      ,[has_vertipaq_optimization]
      ,[generation]
      ,[created_time]
      ,[closed_time]
  FROM [master].[sys].[dm_db_column_store_row_group_physical_stats]
