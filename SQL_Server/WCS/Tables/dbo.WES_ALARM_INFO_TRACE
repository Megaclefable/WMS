/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [TRACE_SEQ]
      ,[TRACE_DTTM]
      ,[TRACE_TYPE]
      ,[SYSTEM_CD]
      ,[WES_ALARM_LEVEL]
      ,[WES_ALARM_CD]
      ,[WES_ALARM_DESC]
      ,[EQP_ID]
      ,[POPUP_YN]
      ,[DB_NAME]
      ,[LOGIN_ID]
      ,[SESSION_IP]
      ,[SESSION_HOST_NAME]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WES_ALARM_INFO_TRACE]
