/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [EQP_TYPE]
      ,[EQP_GROUP]
      ,[CV_POLLING_GROUP]
      ,[EQP_IP]
      ,[EQP_PORT_LIST]
      ,[SC_PLC_ADDR]
      ,[PLC_MAX_ACCESS_LENGTH]
      ,[POLLING_TIME_MSEC]
      ,[CONNECTING_SW_IP]
      ,[CONNECTING_PORT]
      ,[CONNECTIING_DTTM]
      ,[REQ_CONFIG_CHANGE_YN]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
      ,[DEVICE_CODE]
  FROM [WCS].[dbo].[TEST_WCS_EQP_CONNECTION]
