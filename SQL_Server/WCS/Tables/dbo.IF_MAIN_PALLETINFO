/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [IF_DOCNO]
      ,[IFTIME]
      ,[IFFLAG]
      ,[MSGNAME]
      ,[MSGTYPE]
      ,[PALLETID]
      ,[PALLETTYPE]
      ,[REQUESTDATE]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[UPDATE_DTTM]
      ,[UPDATE_BY]
      ,[REMARK]
  FROM [WCS].[dbo].[IF_MAIN_PALLETINFO]
