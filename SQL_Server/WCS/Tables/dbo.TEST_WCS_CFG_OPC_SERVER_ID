/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [OPC_SERVER_CD]
      ,[EQP_TYPE]
      ,[EQP_GROUP]
      ,[CV_POLLING_GROUP]
      ,[OPC_URL]
      ,[OPC_NAMESPACE]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[TEST_WCS_CFG_OPC_SERVER_ID]
