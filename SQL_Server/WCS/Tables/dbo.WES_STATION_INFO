/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [STATION_NO]
      ,[STATION_NM]
      ,[STATION_MODE]
      ,[STATION_PC_IP]
      ,[CV_GROUP]
      ,[REMARK]
      ,[USE_YN]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WES_STATION_INFO]
