/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [OPERATION_KEY]
      ,[PC_IP]
      ,[PC_NAME]
      ,[STATION_NO]
      ,[METHOD_NM]
      ,[OPERATION_NM]
      ,[PARAMETER]
      ,[EQP_ID]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WES_OPERATION_HIST]
