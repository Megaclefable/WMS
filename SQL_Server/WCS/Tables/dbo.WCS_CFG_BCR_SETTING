/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [BCR_ID]
      ,[BCR_TYPE]
      ,[ATTACH_EQP_ID]
      ,[FROM_EQP_ID]
      ,[TO_EQP_ID]
      ,[START_EQP_ID]
      ,[FLOOR]
      ,[SIGNAL_COL]
      ,[DATA_COL]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WCS_CFG_BCR_SETTING]
