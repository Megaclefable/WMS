/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [TRACE_SEQ]
      ,[TRACE_DTTM]
      ,[TRACE_TYPE]
      ,[STORE_ORD]
      ,[STORE_SEQ]
      ,[SRC_SYSTEM]
      ,[PONO]
      ,[POSUBNO]
      ,[MATERIALID]
      ,[MATDESC]
      ,[MATTYPE]
      ,[QTY]
      ,[UNIT]
      ,[STOCK_QTY]
      ,[STOCK_UNIT]
      ,[ACCEPTINSP]
      ,[GD_QTY]
      ,[BD_QTY]
      ,[BATCHNUMBER]
      ,[EFFECTIVEDATE]
      ,[SUPPLIER]
      ,[PRODORDNO]
      ,[PRODORDRELEASEDT]
      ,[SOURCELOCATION]
      ,[DESTLOCATION]
      ,[SERIALNO]
      ,[F_INPUTDATE]
      ,[INPUTDATE]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
      ,[REMARK]
  FROM [WCS].[dbo].[WES_STORE_ORD_DETAIL_TRACE]
