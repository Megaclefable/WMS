/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [EQP_ID]
      ,[HS_WORK_SEQ]
      ,[JOB_SEQ]
      ,[AUTO_RESET_EMPTY_ERR_YN]
      ,[AUTO_RESET_DUPL_ERR_YN]
      ,[MV_IF_SEND_CV_ID]
      ,[MV_IF_RECV_CV_ID]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
      ,[MOVE_SPEED]
  FROM [WCS].[dbo].[WCS_CFG_SC_SETTING]
