/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [TRACE_SEQ]
      ,[TRACE_DTTM]
      ,[TRACE_TYPE]
      ,[EQP_ID]
      ,[ERROR_CODE]
      ,[BCR_RESCAN_FLAG]
      ,[SEND_STATUS]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WCS_COMMAND_CV_REMOTE_TRACE]
