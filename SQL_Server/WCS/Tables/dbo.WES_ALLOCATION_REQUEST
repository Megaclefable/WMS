/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [HOST_ORDER_KEY]
      ,[REQUEST_SEQ]
      ,[HOST_ORDER_TYPE]
      ,[ALLOCATION_REQ_STATE]
      ,[PRIORITY]
      ,[REQUEST_STATION_NO]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
      ,[ERR_CD]
  FROM [WCS].[dbo].[WES_ALLOCATION_REQUEST]
