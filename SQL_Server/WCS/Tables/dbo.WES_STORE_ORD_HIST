/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [STORE_ORD]
      ,[STORE_ORD_DTTM]
      ,[SRC_SYSTEM]
      ,[CONTAINER_ID]
      ,[HOST_ORDER_KEY]
      ,[STORE_TYPE]
      ,[STORE_PRGS_CD]
      ,[STATION_NO]
      ,[BANK]
      ,[BAY]
      ,[LVL]
      ,[PRIORITY]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
      ,[REMARK]
  FROM [WCS].[dbo].[WES_STORE_ORD_HIST]
