/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [TRACE_SEQ]
      ,[TRACE_DTTM]
      ,[TRACE_TYPE]
      ,[WES_ORDER_ID]
      ,[DETAIL_SEQ]
      ,[FROM_EQP_ID]
      ,[TO_EQP_ID]
      ,[CMD_STATUS]
      ,[EQP_CMD_NO]
      ,[REMARK]
      ,[CREATE_DTTM]
      ,[CREATE_BY]
      ,[SAVE_DTTM]
      ,[SAVE_BY]
  FROM [WCS].[dbo].[WES_ORDER_DETAIL_TRACE]
