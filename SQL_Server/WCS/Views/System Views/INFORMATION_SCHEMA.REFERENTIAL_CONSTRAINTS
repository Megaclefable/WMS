/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [CONSTRAINT_CATALOG]
      ,[CONSTRAINT_SCHEMA]
      ,[CONSTRAINT_NAME]
      ,[UNIQUE_CONSTRAINT_CATALOG]
      ,[UNIQUE_CONSTRAINT_SCHEMA]
      ,[UNIQUE_CONSTRAINT_NAME]
      ,[MATCH_OPTION]
      ,[UPDATE_RULE]
      ,[DELETE_RULE]
  FROM [WCS].[INFORMATION_SCHEMA].[REFERENTIAL_CONSTRAINTS]
