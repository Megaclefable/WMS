/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [SEQUENCE_CATALOG]
      ,[SEQUENCE_SCHEMA]
      ,[SEQUENCE_NAME]
      ,[DATA_TYPE]
      ,[NUMERIC_PRECISION]
      ,[NUMERIC_PRECISION_RADIX]
      ,[NUMERIC_SCALE]
      ,[START_VALUE]
      ,[MINIMUM_VALUE]
      ,[MAXIMUM_VALUE]
      ,[INCREMENT]
      ,[CYCLE_OPTION]
      ,[DECLARED_DATA_TYPE]
      ,[DECLARED_NUMERIC_PRECISION]
      ,[DECLARED_NUMERIC_SCALE]
  FROM [WCS].[INFORMATION_SCHEMA].[SEQUENCES]
