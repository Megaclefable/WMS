/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [CONSTRAINT_CATALOG]
      ,[CONSTRAINT_SCHEMA]
      ,[CONSTRAINT_NAME]
      ,[TABLE_CATALOG]
      ,[TABLE_SCHEMA]
      ,[TABLE_NAME]
      ,[CONSTRAINT_TYPE]
      ,[IS_DEFERRABLE]
      ,[INITIALLY_DEFERRED]
  FROM [WCS].[INFORMATION_SCHEMA].[TABLE_CONSTRAINTS]
